# Supabase connection (self-hosted)
SUPABASE_URL=https://api.zenmanager.eu
SUPABASE_SERVICE_KEY=your-service-role-key

# Worker settings
WORKER_POLL_INTERVAL=5
WORKER_SCHEMA=ninja

# Billing (optional - only for hosted SaaS mode)
# Leave unset for self-hosted/open-source (defaults to false)
# Set to 'true' to enable credit-based subscription billing
BILLING_ENABLED=false

# External API keys (also stored in ninja.api_keys table)
OPENAI_API_KEY=
OPENWEBNINJA_API_KEY=
ANYMAIL_FINDER_API_KEY=
RAPIDAPI_MAPS_DATA_API_KEY=
RAPIDAPI_LI_DATA_SCRAPER_KEY=
DATAFORSEO_USERNAME=
DATAFORSEO_PASSWORD=
